/dts-v1/;
/include/ "wm8750.dtsi"

/ {
	model = "VIA APC8750";

	powerkey: pwrkey@0 {
		compatible = "wm,power-keypad";
		interrupt-parent = <&intc0>;
		interrupts = <22>;
		keymap = <116>; /* KEY_POWER */
	};
};

&pinctrl {
	pinctrl-names = "default";
	pinctrl-0 = <&i2c>;

	i2c: i2c {
		wm,pins = <168 169 170 171 172 173>;
		wm,function = <2>;	/* alt */
		wm,pull = <2>;	/* pull-up */
	};
};
